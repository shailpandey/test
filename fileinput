
node {


    stage('SCM') {
    	echo 'Gathering code from version control'
	
    }
    
    stage('Build') {
echo'second.......'

    
def fileList = []

def dir = new File("https://github.com/shailpandey/test/*")
dir.eachFileRecurse (FileType.FILES) { file ->
  fileList << file
}
        
    
}
}
